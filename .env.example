# ImpactOS Environment Variables
# Copy this file to .env and fill in real values. NEVER commit .env.

# Database
DATABASE_URL=postgresql+asyncpg://impactos:changeme@localhost:5432/impactos

# Redis
REDIS_URL=redis://localhost:6379/0

# AI / LLM API Keys
ANTHROPIC_API_KEY=sk-ant-REPLACE_ME
OPENAI_API_KEY=sk-REPLACE_ME
OPENROUTER_API_KEY=sk-or-REPLACE_ME

# Object Storage (local path for dev, s3://bucket for prod)
OBJECT_STORAGE_PATH=./uploads

# Logging
LOG_LEVEL=INFO

# Environment
ENVIRONMENT=dev